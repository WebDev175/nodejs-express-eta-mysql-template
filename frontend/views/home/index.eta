<h1>Countries</h1>

<% const countries = Array.isArray(it.countries) ? it.countries : []; %>

<table>
	<thead>
		<tr>
			<th>Region</th>
			<th>Country</th>
		</tr>
	</thead>
	<tbody>
		<% countries.forEach(country => { %>
		<tr>
			<td><%= country.region_name %></td>
			<td><%= country.country_name %></td>
		</tr>
		<% }) %>
	</tbody>
</table>
